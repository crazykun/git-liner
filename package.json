{
  "name": "git-history-viewer",
  "displayName": "Git History Viewer",
  "description": "显示Git行修改历史和文件修改历史",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "activationEvents": [],
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/crazykun/git-history-viewer.git"
  },
  "bugs": {
    "url": "https://github.com/crazykun/git-history-viewer/issues"
  },
  "homepage": "https://github.com/crazykun/git-history-viewer#readme",

  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gitHistoryViewer.showLineHistory",
        "title": "显示行修改历史",
        "category": "Git History"
      },
      {
        "command": "gitHistoryViewer.showFileHistory",
        "title": "显示文件修改历史",
        "category": "Git History"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "gitHistoryViewer.showLineHistory",
          "when": "editorHasSelection",
          "group": "navigation"
        },
        {
          "command": "gitHistoryViewer.showFileHistory",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  }
}